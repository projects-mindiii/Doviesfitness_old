<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:id="@+id/rootLayoutnew"
    android:layout_height="wrap_content"
     >

    <TextView
        style="@style/AllHeadingsetandreps2"
        android:id="@+id/tv_sets_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="@dimen/_11sdp"
        android:text="SET 1"
        android:fontFamily="@font/netflix_sans_regular"
        android:textAllCaps="true"
        android:textSize="@dimen/_10sdp"
        android:textColor="@color/colorWhite"
        />
    <ImageView
        android:id="@+id/ic_set_completed_tick"
        android:layout_width="@dimen/_15sdp"
        android:layout_height="@dimen/_15sdp"
        android:visibility="gone"
        android:layout_toEndOf="@+id/tv_sets_name"
        android:src="@drawable/check_right"
        android:layout_centerVertical="true"
        android:layout_marginStart="-16dp"
        />

</RelativeLayout>